            <script>
                function signout() {
		  onLoad();
                  if(gapi.auth2){
                    console.log("we now gapi")
                    var auth2 = gapi.auth2.getAuthInstance();
                    auth2.signOut().then(function () {
                      console.log('User signed out.');
                      localStorage.clear();
                      window.location.assign("../back/");

                    });
                    window.location.assign("../back/");
                  }
                }

                function onLoad() {
                  gapi.load('auth2', function() {
                    gapi.auth2.init();
                  });
                }
              </script>
        <script>
