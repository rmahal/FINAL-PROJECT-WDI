      <br>
      <h1 style="margin-left:25%;">Tag Name: <%= tag.TagName %> </h1>
      <hr>
      <br>
    
      <div class="container result-listing">
          <div class="row">
            <div class="col-md-12">
                <div class="result_count">
                  <!-- <p>About 19,10,00,000 results (0.70 seconds)</p> -->
                </div>
          <ul class="list-unstyled">
            <% for(let i = 0 ; i < response.length; i++) { %>
              <li class="media" href="../userprofile/<%=response[i].user._id %>">
                  <a href="../userprofile/<%=response[i].user._id %>">
                    <img class="mr-3 rounded-circle" src="../<%=response[i].userExt.PhotoURL %>" height='100' width='100' alt="Generic placeholder image">
                  </a>
                  <div class="media-body">
                    <a href="../userprofile/<%=response[i].user._id%>" class="mt-0 mb-1 name">
                      <span class="first_name"><%=response[i].user.FirstName %></span>
                      <span class="last_name"> <%=response[i].user.LastName %></span>
                    </a>
                    <div class="more-info">
                      <span class="desigation">
                        <a><%=response[i].user.JobTitle %></a></span>,<span class="company_name">
                          <a><%=response[i].user.Org %></a>
                        </span>
                      </div>
                      <div class="contact-info">
                        <span class="country">
                          <a>
                            <span>
                              <img src="../img/united-states.svg"></span> <%=response[i].user.CountryCode %> </a></span>
                              <span class="email">
                                <a href="mailto:<%=response[i].user.Email %>">
                                  <span>
                                    <img src="../img/email.svg"></span> <%=response[i].user.Email %> </a></span>
                                    <span class="phone">
                                      <a href="tel:1<%=response[i].user.Phone %>">
                                        <span>
                                          <img src="../img/phone.svg"></span> +1 <%=response[i].user.Phone %> </a></span>
                                        </div>
                                      </div>
                                          <a href="userprofile/<%=response[i].user._id%>" class="visit_profile_button">
                                            <img src="../img/next.svg">
                                          </a>
                                        </li>
                                        <hr>
                                      
            <% } %>
          </ul>
        </div>
      </div>
 </div>  




      
